{
  "name": "power.components",
  "version": "3.0.0",
  "description": "Pow3r v3 Component Library with Full Automation System",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./automation": {
      "import": "./dist/lib/automation/index.js",
      "types": "./dist/lib/automation/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:minimal": "vite build --config vite.config.minimal.ts",
    "build:watch": "tsc --watch && vite build --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:components": "vitest run --reporter=verbose src/components",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:api": "node src/lib/automation/automation-cli.js test api",
    "test:integration": "node src/lib/automation/automation-cli.js test e2e",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "verify": "node src/lib/automation/automation-cli.js verify full",
    "verify:full": "node src/lib/automation/automation-cli.js verify full",
    "verify:constitutional": "node src/lib/automation/automation-cli.js verify constitutional",
    "verify:schemas": "node src/lib/automation/automation-cli.js verify schemas",
    "verify:components": "node src/lib/automation/automation-cli.js verify components",
    "verify:final": "node src/lib/automation/automation-cli.js verify final",
    "automation": "node src/lib/automation/automation-cli.js",
    "automation:start": "node src/lib/automation/automation-cli.js start --monitor",
    "automation:stop": "node src/lib/automation/automation-cli.js stop",
    "automation:status": "node src/lib/automation/automation-cli.js status",
    "automation:process": "node src/lib/automation/automation-cli.js process-request",
    "automation:report": "node src/lib/automation/automation-cli.js report automation",
    "ateam:start": "node src/lib/automation/ateam-cli.js start",
    "ateam:stop": "node src/lib/automation/ateam-cli.js stop",
    "ateam:status": "node src/lib/automation/ateam-cli.js status",
    "ateam:process": "node src/lib/automation/ateam-cli.js process",
    "ateam:report": "node src/lib/automation/ateam-cli.js report",
    "ateam:dashboard": "node src/lib/automation/ateam-cli.js dashboard",
    "github:create-pr": "node src/lib/automation/automation-cli.js github create-pr",
    "github:merge-pr": "node src/lib/automation/automation-cli.js github merge-pr",
    "github:cleanup": "node src/lib/automation/automation-cli.js github cleanup",
    "deploy:preview": "node src/lib/automation/automation-cli.js deploy preview",
    "deploy:production": "node src/lib/automation/automation-cli.js deploy production",
    "deploy:force": "node src/lib/automation/automation-cli.js deploy production --force",
    "screenshot": "node src/lib/automation/automation-cli.js screenshot",
    "screenshot:production": "node src/lib/automation/automation-cli.js screenshot https://power-components.com",
    "cleanup:repository": "node src/lib/automation/automation-cli.js github cleanup",
    "organize:structure": "node scripts/organize-repo-structure.js",
    "docs:update": "node src/lib/automation/automation-cli.js docs update",
    "docs:generate": "node src/lib/automation/automation-cli.js docs generate",
    "report:completion": "node src/lib/automation/automation-cli.js report automation --format json > reports/completion-report.json",
    "report:status": "node src/lib/automation/automation-cli.js status --json > reports/status-report.json",
    "agents:list": "node src/lib/automation/automation-cli.js agents list",
    "agents:status": "node src/lib/automation/automation-cli.js agents status",
    "migrate:v3": "node scripts/migrate-to-v3.js",
    "migrate:v2-to-v3": "node src/lib/migration/migrate-cli.js migrate",
    "migrate:status": "node src/lib/migration/migrate-cli.js status",
    "deploy:components": "node src/lib/automation/deploy-components.js",
    "build:all": "npm run build && npm run build:power-canvas && npm run build:power-redact",
    "build:power-canvas": "cd power-canvas && npm run build",
    "build:power-redact": "cd power-redact && npm run build",
    "install:all": "npm install && cd power-canvas && npm install && cd ../power-redact && npm install",
    "postinstall": "npm run type-check && npm run lint",
    "precommit": "npm run lint && npm run type-check && npm run test:run",
    "prepush": "npm run verify:full && npm run test:e2e"
  },
  "keywords": [
    "react",
    "components",
    "typescript",
    "tailwind",
    "automation",
    "pow3r",
    "v3",
    "x-files",
    "self-healing",
    "constitutional-compliance"
  ],
  "author": "Pow3r Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/memorymusicllc/power.components.git"
  },
  "bugs": {
    "url": "https://github.com/memorymusicllc/power.components/issues"
  },
  "homepage": "https://power-components.com",
  "dependencies": {
    "@vitest/ui": "^1.0.0",
    "c8": "^8.0.1",
    "class-variance-authority": "^0.7.0",
    "cli-table3": "^0.6.5",
    "clsx": "^2.0.0",
    "commander": "^11.1.0",
    "framer-motion": "^12.23.24",
    "immer": "^10.1.3",
    "lucide-react": "^0.294.0",
    "ora": "^9.0.0",
    "playwright": "^1.40.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "react-redux": "^9.0.4",
    "react-three-fiber": "^6.0.13",
    "recharts": "^2.8.0",
    "redux": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.6",
    "vitest": "^1.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "pow3r": {
      "version": "3.0.0",
      "constitution": "pow3r.v3.law.md",
      "automation": {
        "enabled": true,
        "agents": 10,
        "monitoring": true,
        "selfHealing": true
      }
    }
  }
}
