      "type": "ui.showcase",
      "version": "3.0.0",
      "io": {
        "inputs": [
          { "name": "searchQuery", "dtype": "string", "validationRule": "isString" },
          { "name": "filterTags", "dtype": "array", "validationRule": "isArrayOfStrings" },
          { "name": "selectedPhase", "dtype": "string", "validationRule": "isOneOf:Phase 1,Phase 2,Core" }
